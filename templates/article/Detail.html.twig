	<script>
		function addFav() {
			let $e = $('#fav');
			let $action = $e.data("action")
			console.log($action)
			$.ajax({
				url: $action,
				type: 'POST',
				data: {},
				async: true,
				success: function (data, status) {
					if($e.data('filled') === 'filled'){
						$e.html('<i class="fa fa-heart-o"></i>')
						$e.data('filled', 'empty')
					} else {
						$e.html('<i class="fa fa-heart"></i>')
						$e.data('filled', 'filled')
					}
				},
				error: function (xhr, textStatus, errorThrown) {
					alert('Error occurred try again.');
				}
			});
		}
	</script>
	}
